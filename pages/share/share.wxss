/* pages/share/share.wxss */
page{
 background: #f5f5f5;
}
.s-container {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}

.s-container .item {
  background: white;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 45%;
  margin-left: 3.33%;
  margin-top: 25rpx;
  border-radius: 8rpx;
}

.s-container .item .image {
  width: 100%;
  height: 350rpx;
  border-radius: 8rpx 8rpx 0rpx 0rpx;
}
